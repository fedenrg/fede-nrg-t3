{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "labels": [
    "dependencies"
  ],
  "rangeStrategy": "bump",
  "hostRules": [
    {
      "matchHost": "https://npm.pkg.github.com/",
      "hostType": "npm",
      "encrypted": {
        "npmToken": ""
      }
    }
  ],
  "npmrc": "@fedeviotti:registry=https://npm.pkg.github.com/:_authToken=${NPM_TOKEN}"
}
